<launch>
    <!-- Launch move_group configuration for interacting with the simulator -->
    <remap from="joint_states" to="simulation/joint_states"/>
    
    <node name="robot_state_publisher" 
	  pkg="robot_state_publisher"
	  type="robot_state_publisher"
	  respawn="true"
	  output="screen" />
    
    <include file="$(find uibk_robot_moveit_config)/launch/move_group_uibk.launch">
	<arg name="allow_trajectory_execution" value="true" />
	<arg name="publish_monitored_planning_scene" value="true" />
	<arg name="moveit_sensor_manager" value="simulation" /> 
	<arg name="moveit_controller_manager" value="simulation" />
    </include>   
</launch>
