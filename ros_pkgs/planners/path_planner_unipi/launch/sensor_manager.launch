<launch>

  <!-- This file makes it easy to include the settings for sensor managers -->  

  <!-- Load the robot specific sensor manager; this sets the moveit_sensor_manager ROS parameter -->
  <arg name="moveit_sensor_manager" default="kuka_lwr_assembled" />
  <include file="$(find path_planner_unipi)/launch/$(arg moveit_sensor_manager)_moveit_sensor_manager.launch" />
  
</launch>
